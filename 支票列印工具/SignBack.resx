            xrTableCell7.Text = "備註欄";
}

private void xrEndDate_BeforePrint(object sender, System.Drawing.Printing.PrintEventArgs e) {
		if (xrEndDate.Text.Length == 8)
            {

                string y = xrEndDate.Text.Substring(0, 4);
                string m = xrEndDate.Text.Substring(4, 2);
                string d = xrEndDate.Text.Substring(6, 2);


                DateTime dt = new DateTime();
                if (DateTime.TryParse(y + "/" + m + "/" + d, out dt))
                {
                       xrEndDate.Text = dt.AddYears(-1911).Year.ToString() + dt.ToString("/MM/dd");
                }
            }
}
